{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "ts:check": "tsc",
    "format:check": "npx prettier --check \"**/*.{js,jsx,json,ts,tsx}\"",
    "format": "npx prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
    "lint": "npx eslint \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run ts:check && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "npx eslint",
    "**/*.{js,jsx,json,ts,tsx}": "npx prettier --write"
  },
  "dependencies": {
    "expo": "~50.0.17",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/js": "^9.1.1",
    "@types/react": "~18.2.45",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "private": true
}
